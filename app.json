{
  "expo": {
    "name": "Zara Shift Tracker",
    "slug": "zara-shift-tracker",
    "version": "1.3.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "zara-shift-tracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thisisbinh.zarashifttracker",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thisisbinh.zarashifttracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "614958e8-bc6d-45b9-bcdf-b3a67b3798f9"
      }
    },
    "runtimeVersion": "1.3.5",
    "updates": {
      "url": "https://u.expo.dev/614958e8-bc6d-45b9-bcdf-b3a67b3798f9"
    },
    "router": {
      "ignoreRoutes": ["types", "utils", "contexts"]
    }
  }
}
